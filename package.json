{
  "name": "xstate-typescript-template",
  "version": "1.0.0",
  "description": "You can fork this sandbox to create a reproduction for XState issues.",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open",
    "watch": "xstate typegen \"src/**/*.ts?(x)\" --watch",
    "build": "parcel build index.html",
    "postinstall": "xstate typegen \"./src/**/*.ts?(x)\""
  },
  "dependencies": {
    "xstate": "4.32.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@xstate/cli": "^0.3.3",
    "parcel-bundler": "^1.6.1",
    "typescript": "^4.8.3"
  },
  "keywords": []
}
